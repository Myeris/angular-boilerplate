{
  "name": "angular-boilerplate",
  "version": "1.0.0",
  "author": "Marie Foussette",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "build:dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "build:production": "npm run clean && npm run build",
    "clean": "rimraf build",
    "start": "npm run build:dev",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "test": "node --max_old_space_size=4096 node_modules/karma/bin/karma start"
  },
  "devDependencies": {
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@compodoc/compodoc": "^1.1.2",
    "@ngtools/webpack": "1.10.0",
    "@types/core-js": "0.9.42",
    "@types/jasmine": "2.5.46",
    "@types/karma": "0.13.34",
    "@types/node": "^9.6.21",
    "angular-router-loader": "0.8.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.5.0",
    "cross-env": "^5.1.4",
    "file-loader": "^1.1.9",
    "html-loader": "0.5.5",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "2.0.3",
    "node-sass": "^4.8.3",
    "raw-loader": "0.5.1",
    "rimraf": "~2.6.2",
    "sass-loader": "^6.0.6",
    "typescript": "~2.7.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "~2.11.1"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "6.1.0",
    "@types/vexflow": "1.2.31",
    "core-js": "^2.5.4",
    "raven-js": "^3.26.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "tslib": "1.7.1",
    "zone.js": "^0.8.23"
  }
}
